<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlashButton">
        <attr name="state_flash_enabled" format="boolean"/>
    </declare-styleable>

    <attr name="title" format="string"/>

    <declare-styleable name="EditTextWithTitle">
        <attr name="title"/>
        <attr name="enabled" format="boolean"/>
        <attr name="inputType" format="string"/>
    </declare-styleable>

    <declare-styleable name="SpinnerWithTitle">
        <attr name="title"/>
        <attr name="values" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FingerSelectionRow">
        <attr name="showHeader" format="boolean"/>
        <attr name="row_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="ListWithLoadingSpinner">
        <attr name="error_text" format="string"/>
    </declare-styleable>

</resources>